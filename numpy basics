#creation of arrays using numpy
import numpy as np
x=np.array([1,2,3])
print(x)
y=np.array([[1,2,3],[4,5,6]],dtype=float)
print(y)
z=np.linspace(10,20,6)
print(z)
q=np.arange(10,20,2)
print(q)
u=np.zeros([3,4],dtype=int)
print(u)
#properties of arrays
print(x.shape,y.shape,z.shape,q.shape,u.shape)
print(x.ndim,y.ndim,z.ndim,q.ndim,u.ndim)
print(x.size,y.size,z.size,q.size,u.size)
print(type(x))
print(x.itemsize)
#operations on arrays
print(x+y)
print(x*y)
print(x//y)
print(x-y)
